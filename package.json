{
  "name": "todolist",
  "version": "1.0.0",
  "description": "A lightweight Todo List application",
  "main": "src/main/index.js",
  "scripts": {
    "start": "concurrently \"webpack serve --mode development\" \"electron .\"",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "webpack --mode production",
    "build": "npm run build:main && npm run build:renderer",
    "dev:renderer": "webpack serve --mode development",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run dev:renderer\" \"npm run build:main && electron .\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "docs": "typedoc",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder"
  },
  "author": "",
  "license": "ISC",
  "build": {
    "appId": "com.todolist.app",
    "productName": "Todo List",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.productivity"
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__react": "^10.0.1",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "electron": "^37.2.4",
    "electron-builder": "^26.0.12",
    "html-webpack-plugin": "^5.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.4.2",
    "typedoc": "^0.28.7",
    "typedoc-plugin-markdown": "^4.7.1",
    "typescript": "^5.0.4",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-date-pickers": "^8.9.0",
    "@types/react-calendar": "^3.9.0",
    "date-fns": "^4.1.0",
    "react": "^18.2.0",
    "react-calendar": "^6.0.0",
    "react-dom": "^18.2.0"
  }
}
